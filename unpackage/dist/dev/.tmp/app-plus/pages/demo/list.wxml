s="action text-blue" bindtap="__e">取消</view></view><view class="cu-list menu"><block wx:for="{{btnGroup}}" wx:for-item="btn" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['call',['$0'],[[['btnGroup','',index,'handle']]]]]]]}}" class="cu-item" bindtap="__e"><view class="content"><text class="{{['text-grey',btn.icon?'cuIcon-'+btn.icon:'']}}"></text><text class="text-grey text-lg">{{btn.text}}</text></view></view></block></view></view></view></block><block wx:if="{{showTopModal}}"><view data-event-opts="{{[['touchmove',[['moveHandle',['$event']]]],['tap',[['toggleTopModel',['$event']]]]]}}" class="cu-modal top-modal show" catchtouchmove="__e" bindtap="__e"><view class="cu-dialog" style="{{(topModalStyle)}}"><view class="cu-list menu"><block wx:for="{{btnGroupTop}}" wx:for-item="btn" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['call',['$0'],[[['btnGroupTop','',index,'handle']]]]]]]}}" class="cu-item" catchtap="__e"><view class="content"><text class="{{['text-grey',btn.icon?'cuIcon-'+btn.icon:'']}}"></text><text class="text-grey text-lg">{{btn.text}}</text></view></view></block></view></view></view></block><block wx:if="{{showFilterModal}}"><view data-event-opts="{{[['touchmove',[['moveHandle',['$event']]]],['tap',[['toggleFilterModel',['$event']]]]]}}" class="cu-modal top-modal show" catchtouchmove="__e" bindtap="__e"><view class="cu-dialog" style="{{(topModalStyle)}}"><view class="cu-list menu"><block wx:for="{{btnGroupFilter}}" wx:for-item="btn" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['call',['$0'],[[['btnGroupFilter','',index,'handle']]]]]]]}}" class="cu-item" catchtap="__e"><view class="content"><text class="{{['text-grey',btn.icon?'cuIcon-'+btn.icon:'']}}"></text><text class="text-grey text-lg">{{btn.text}}</text></view></view></block></view></view></view></block></view>